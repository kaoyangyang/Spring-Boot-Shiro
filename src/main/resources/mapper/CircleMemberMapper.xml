<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.infotop.mapper.CircleMemberMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.infotop.entity.CircleMember">
		<id column="id" property="id" />
		<result column="circle_id" property="circleId" />
		<result column="user_id" property="userId" />
		<result column="join_time" property="joinTime" />
		<result column="join_status" property="joinStatus" />
		<result column="user_type" property="userType" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, circle_id AS circleId, user_id AS userId, join_time AS joinTime, join_status AS joinStatus, user_type AS userType
    </sql>

</mapper>
